<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.designer.DesignerMapper">
	<insert id="dcreate" parameterType="com.study.designer.DesignerDTO">
		insert into designer(did,dpw,dname,birth,demail,dphone,address1,address2,dzipcode) 
		values(#{did},#{dpw},#{dname},#{birth},#{demail},#{dphone},#{address1},#{address2},#{dzipcode})
		
	</insert>

	<insert id="lcreate" parameterType="com.study.designer.LicenseDTO">
		
	</insert>
</mapper>